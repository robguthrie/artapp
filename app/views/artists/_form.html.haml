= form_for @artist do |f|
  = f.error_messages
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :description
    %br
    = f.text_field :description
  %p
    = f.label :phone
    %br
    = f.text_field :phone
  %p
    = f.label :email
    %br
    = f.text_field :email
  %p
    = f.submit
